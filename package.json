{
  "name": "vite5-vue-ssr",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu NODE_ENV=development nodemon -r dotenv/config server/app.js",
    "build": "npm run build:client && npm run build:server && npm run build:app",
    "build:client": "vite build --ssrManifest --outDir server/public/client",
    "build:server": "vite build --ssrManifest --ssr ./src/entry-server.js --outDir server/public/server",
    "build:app": "vite build -c vite.config.build.js --ssr server/app.js --outDir dist",
    "preview": "cross-env NODE_ENV=production node server",
    "start": "cross-env NODE_ENV=production node ./dist/app.js"
  },
  "dependencies": {
    "@lucky-canvas/vue": "^0.1.11",
    "@vue/babel-helper-vue-jsx-merge-props": "^1.4.0",
    "async-lock": "^1.2.4",
    "axios": "^1.7.1",
    "blueimp-md5": "^2.16.0",
    "compression": "^1.7.4",
    "core-js": "^3.6.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "i18n": "^0.15.0",
    "js-sha256": "^0.9.0",
    "patpat-ui": "^1.3.6",
    "recaptcha-v3": "^1.8.0",
    "register-service-worker": "^1.7.1",
    "sirv": "^2.0.4",
    "swiper": "^5.4.5",
    "ts-node": "^10.9.2",
    "uuid": "^9.0.1",
    "vue": "^2.7.0",
    "vue-client-only": "^2.1.0",
    "vue-i18n": "^8.17.3",
    "vue-l10n": "^1.2.4",
    "vue-lazyload": "^1.3.3",
    "vuex": "^3.4.0",
    "vuex-router-sync": "^5.0.0",
    "web-vitals": "^3.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue2": "^2.3.1",
    "@vitejs/plugin-vue2-jsx": "^1.1.1",
    "@vue/babel-preset-jsx": "^1.4.0",
    "@vue/compiler-sfc": "^3.4.27",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "cross-env": "^7.0.3",
    "i": "^0.3.7",
    "less": "^4.2.0",
    "plugin-vue2-jsx-vite5": "^1.0.0",
    "rollup-plugin-copy": "^3.5.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.0",
    "vue-tsc": "^2.0.19"
  }
}
